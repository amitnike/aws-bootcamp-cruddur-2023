#! /user/bin/bash

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="connect-to-service"
printf "${CYAN}== ${LABEL}${NO_COLOR}\n"

aws ecs execute-command  \
--region ap-south-1 \
--cluster cruddur \
--task b28eeb76c7ac435b90ef7976020def62 \
--container backend-flask \
--command "/bin/bash" \
--interactive