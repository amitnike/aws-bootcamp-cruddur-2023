#! /user/bin/bash
-e # stop if it fails at any point

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="db-setup"
printf "${CYAN}== ${LABEL}${NO_COLOR}\n"


DB_PATH=$(pwd)
echo $(pwd)

source "$DB_PATH/bin/db/drop"
source "$DB_PATH/bin/db/create"
source "$DB_PATH/bin/db/schema-load"
DB_PATH=$(pwd)
echo $(pwd)
source "$DB_PATH/bin/db/seed"
python "$DB_PATH/bin/db/update_cognito_user_ids"